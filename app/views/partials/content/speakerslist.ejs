<article class="speakerslist">
  <div class="container">
    <div class="row">
        <% speakers.forEach(function(item) { %>
        <div class="col-sm-8">
          <h3 class="speakerslist-title"><%= item.title %></h3>
          <h5 class="speakerslist-name">with <a href="/speakers/<%= item.name %>"><%= item.name %></a></h5>
          <p class="speakerslist-info"><a href="/speakers/<%= item.name %>">
            <img class="speakerslist-img img-circle pull-left" src="/images/speakers/<%= item.shortname %>.png" alt="Photo of <%= item.name %>"></a>
            <p><%= item.summary %></p>
          </a></p>
        </div>
      <% }); %>
    </div>
  </div>
</article>
